{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/test-data-sync.html",
      "dest": "/test-data-sync.html"
    },
    {
      "src": "/test-(.*)",
      "dest": "/test-$1.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
